#! /usr/bin/env node
/**
 * @file        prod-sched.simple
 *              Test that show that we can load a config from the production scheduler
 *
 * @author      Wes Garland, wes@kingsds.network
 * @date        Aug 2020
 */
process.env.DCP_SCHEDULER_LOCATION = 'https://scheduler.distributed.computer/';

function main()
{
  const { assert } = require('dcp/dcp-assert').always;
  
  assert(dcpConfig.scheduler.location.href === process.env.DCP_SCHEDULER_LOCATION);
  assert(dcpConfig.scheduler.services);
  assert(Object.keys(dcpConfig.scheduler.services).length > 5);
}

require('..').init().then(main);
